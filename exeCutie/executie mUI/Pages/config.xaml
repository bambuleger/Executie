<UserControl x:Class="executie_mUI.Pages.ListPage1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab Layout="List">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="defensive cooldowns" Source="/pages/config/defCDs.xaml"/>
                <mui:Link DisplayName="offensive cooldowns" Source="/pages/config/offCDs.xaml"/>
                <mui:Link DisplayName="misc" Source="/pages/config/misc.xaml"/>
                <mui:Link DisplayName="raid events" Source="/pages/config/raid_events.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
