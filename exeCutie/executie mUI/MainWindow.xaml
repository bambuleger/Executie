<mui:ModernWindow x:Class="executie_mUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="mui" IsTitleVisible="True"          
        ContentSource="/Pages/Home.xaml">
    <mui:ModernWindow.TaskbarItemInfo>
        <TaskbarItemInfo/>
    </mui:ModernWindow.TaskbarItemInfo>

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="executie"/>
        <mui:LinkGroup DisplayName="settings">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="general"/>
                <mui:Link DisplayName=""/>
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup DisplayName="support"/>
    </mui:ModernWindow.MenuLinkGroups>

    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="settings" Source="/Pages/Settings.xaml" />
    </mui:ModernWindow.TitleLinks>
</mui:ModernWindow>
